<div id="pageTitle">
	<h1><%= @serial.name%></h1>
</div>

<div id="toolbar">
        <%= link_to "Back", :action => :index %>
        <%= link_to "Edit", :action => :edit, :id => @serial.object %>
</div>


<div id="content">

    <table>
      
      <tr>
    	<td class="itemLabel">Name</td>
        <td class="itemValue"><%= @serial.name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Description</td>
        <td class="itemValue"><%= @serial.description%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">BrandId</td>
        <td class="itemValue"><%= @serial.brand_id%></td>
      </tr>
      
    </table>
</div>