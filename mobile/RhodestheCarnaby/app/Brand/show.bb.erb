<div id="pageTitle">
	<h1><%= @brand.name%></h1>
</div>

<div id="toolbar">
        <%= link_to "Back", :action => :index %>
        <%= link_to "Edit", :action => :edit, :id => @brand.object %>
</div>


<div id="content">

    <table>
      
      <tr>
    	<td class="itemLabel">Name</td>
        <td class="itemValue"><%= @brand.name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Description</td>
        <td class="itemValue"><%= @brand.description%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Logo</td>
        <td class="itemValue"><%= @brand.logo%></td>
      </tr>
      
    </table>
</div>